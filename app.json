{
  "expo": {
    "name": "boonApp",
    "slug": "boonApp",
    "scheme": "boonapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "hermes": {
      "enabled": true,
      "commandUnlink": "node node_modules/hermes-engine/unlink"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.idsmoder.boonApp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "@config-plugins/react-native-blob-util",
      "@config-plugins/react-native-pdf",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow boonApp to access your camera",
          "microphonePermission": "Allow boonApp to access your microphone",
          "cameraRollPermission": "Allow boonApp to access your camera roll",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "9067f788-b020-4b44-a0cc-176e520730e2"
      }
    }
  }
}
